{
  "name": "@zesty-io/cli",
  "description": "CLI for interacting with Zesty.io services",
  "version": "0.0.1",
  "author": "Zesty.io",
  "contributors": [
    "Stuart Runyan<stuart@zesty.io>"
  ],
  "bin": {
    "zesty": "./bin/run"
  },
  "bugs": {
    "url": "https://github.com/zesty-io/cli/issues"
  },
  "dependencies": {
    "@oclif/command": "^1.5.19",
    "@oclif/config": "^1.13.3",
    "@oclif/plugin-autocomplete": "^0.1.4",
    "@oclif/plugin-help": "^2.2.1",
    "@oclif/plugin-not-found": "^1.2.3",
    "@oclif/plugin-warn-if-update-available": "^1.7.0",
    "@zesty-io/sdk": "0.0.9",
    "chalk": "^2.4.2",
    "cli-ux": "^5.3.3",
    "listr": "^0.14.3",
    "listr-input": "^0.2.0",
    "node-notifier": "^8.0.1",
    "tslib": "^1.10.0"
  },
  "devDependencies": {
    "@oclif/dev-cli": "^1.22.2",
    "@oclif/tslint": "^3.1.1",
    "@types/node": "^10.17.4",
    "globby": "^10.0.1",
    "ts-node": "^8.4.1",
    "tslint": "^5.20.1",
    "typescript": "^3.7.2"
  },
  "engines": {
    "node": ">=10.0.0"
  },
  "files": [
    "/bin",
    "/lib",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json"
  ],
  "homepage": "https://github.com/zesty-io/cli#readme",
  "keywords": [
    "oclif"
  ],
  "license": "ISC",
  "main": "index.js",
  "oclif": {
    "commands": "./lib/commands",
    "bin": "zesty",
    "plugins": [
      "@oclif/plugin-help",
      "@oclif/plugin-autocomplete",
      "@oclif/plugin-not-found",
      "@oclif/plugin-warn-if-update-available"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/zesty-io/cli.git"
  },
  "scripts": {
    "postpack": "rm -f oclif.manifest.json",
    "posttest": "tslint -p . -t stylish",
    "prepack": "rm -rf lib && tsc -b && oclif-dev manifest && oclif-dev readme",
    "test": "echo NO TESTS",
    "version": "oclif-dev readme && git add README.md"
  },
  "types": "lib/index.d.ts"
}
